<% content_for :title, "Posts" %>

<h1>Posts</h1>

<div id="posts">
  <% @posts.each do |post| %>
    <%= render post %>
    <p>
      <% if post.user == current_user %>
      <%= link_to "Show this post", post %>
      <% end %>
    </p>
  <% end %>
</div>

<%= link_to "New post", new_post_path %>
